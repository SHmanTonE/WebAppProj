@model IList<IGrouping<string, MasterProj.Models.Author>>

@{
    ViewBag.Title = "groupbyresult";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Group By Author City</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Author City:
        </th>
        <th>
            Number of Authors:
        </th>

    </tr>

    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>
                @Model[i].Key
            </td>
            <td>
                @Model[i].Count()
            </td>

        </tr>
    }

</table>
